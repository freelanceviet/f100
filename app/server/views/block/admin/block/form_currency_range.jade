.row
	.col-lg-6
		form(role="form", method="post", action="/store_currency_range", id="form_admin_currency_range_add", enctype="multipart/form-data;utf-8")
			.form-group
				lable Range name EN
				input(class="form-control", name="admin_range_name_en")
			.form-group
				lable Range name VN
				input(class="form-control", name="admin_range_name_vn")
			.form-group
				lable From
				input(class="form-control", name="admin_range_from")
			.form-group
				lable To
				input(class="form-control", name="admin_range_to")
			.form-group
				lable Is default
				input(class="form-control", name="admin_is_default", value="0")
			input(type="submit", class="btn btn-default", value="Submit Button", id="bt_admin_currency_range_submit")