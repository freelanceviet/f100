.container
	h2(class="titleMtn") Popular competition
	ul(class="popu")
		-	for(var i=0; i<resContests.length; i++)
				li
					.ctnPopu
						.arThumb
							a(href="/contestdetail?id="+resContests[i]._id+"&tab=timeline&value="+resContests[i].project_name.replace(/ /g,"-")+"", class="thumbPopu")
								img(src="images/thumb.jpg", alt="Web developer")
							.hvCatePopu
								a(href="/contestdetail?id="+resContests[i]._id+"&tab=timeline&value="+resContests[i].project_name.replace(/ /g,"-")+"")= resContests[i].job_description
						.txtCatePopu
							.lfCtePpu
								a(href="/contestdetail?id="+resContests[i]._id+"&tab=timeline&value="+resContests[i].project_name.replace(/ /g,"-")+"", class="titCatePopu")= resContests[i].project_name
								p $1000 USD
							.rtCtePpu
								p GJD
	.vMore
		a(href="/contests", class="btnmore") View more