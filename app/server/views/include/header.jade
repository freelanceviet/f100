	.boxSup
		a(href="#inlineBonusInfoUser", class="ctnIn show_box_bonus_info_user") ---
		#inlineBonusInfoUser.ctnSup
			form(class="fmSup", method="post", action="/storeUserUpdateFacebook", id="register_update_face_form", enctype="multipart/form-data;utf-8")
				.supFb
					h2(class="titleMtn")
					p  Update info to continue with page!
					p(class="clGrey") Sign up with Facebook and discover what your friend are doing
					.fbGetjobdone
					span(class="lineSup")
						em Or
				fieldset(class="gridSup")
					label Username
					input(class="ipSup", tabindex="3", name="reg-user-name-tf", id="reg-user-name-tf")
					label(class="errSup", id="reg-user-name-cf", style="display:none") Enter your username
				fieldset(class="gridSup")
					label Email
					input(class="ipSup", tabindex="4", name="reg-email-tf", id="reg-email-tf")
					label(class="errSup", id="reg-email-cf", style="display:none") Enter your email
				fieldset(class="gridSup")
					label Password
					input(type="password", class="ipSup", tabindex="5", name="reg-pass-tf", id="reg-pass-tf")
					label(class="errSup", id="reg-pass-cf", style="display:none") Enter your password
				.ckService
					h5 What are you looking for?
					ul(class="lst_ckSup")
						li(class="ck_sup")
							input(type="radio", id="buySer", name="f_type[]", value="1")
							label(for="buySer") Buy Services
						li(class="ck_sup")
							input(type="radio", id="selSer", name="f_type[]", value="2", checked)
							label(for="selSer") Sell Services
						li(class="ck_sup", style="display:none")
							input(type="radio", id="both_ckSup", name="f_type[]", value="0")
							label(for="both_ckSup") Both
				fieldset
					button(type="submit", class="btnsubmit", id="rp-bt-action-form-update") Sign up
	.boxLogin
		#inlineLogin.ctnSup
			form(class="fmSup", method="post",action="/login", id="login-form", enctype="multipart/form-data;utf-8")
				.supFb
					.sf_loading
					h2(class="titleMtn clorg", style="display:none") Please login
					p Donâ€™t have account? Sign up
					div(class="btnSupFb", onclick="checkLoginState();") Login with facebook
					span(class="lineSup")
						em Or
				fieldset(class="gridSup")
					label Email
					input(class="ipSup",  tabindex="1", name="lo-email-tf", id="lo-email-tf")
					label(class="errSup", id="lo-email-cf", style="display:none") Enter your email
				fieldset(class="gridSup")
					label Password
					input(class="ipSup", tabindex="2", name="lo-pass-tf", id="lo-pass-tf", type="password")
					label(class="errSup", id="lo-pass-cf", style="display:none") Enter your password
				.ckService
					ul(class="lst_ckSup")
						li(class="ck_sup nmag")
							input(type="checkbox", id="remem", name="ckSup")
							label(for="remem") Remember me
						li(class="ck_sup nmag pullFlt_right")
							a(class="fget_pass", href="") Forgot password
				.fieldset
					button(type="submit", class="btnsubmit") Login
	.boxSup
		#inlineSup.ctnSup
			form(class="fmSup", method="post", action="/storeUser", id="register_form", enctype="multipart/form-data;utf-8")
				.supFb
					.sf_loading
					h2(class="titleMtn")
					p  Already have an account? Login
					p(class="clGrey") Sign up with Facebook and discover what your friend are doing
					div(class="btnSupFb", onclick="checkLoginState();") Login with facebook
					.fbGetjobdone
					span(class="lineSup")
						em Or
				fieldset(class="gridSup")
					label First Name
					input(class="ipSup", tabindex="1", name="reg-first-name-tf", id="reg-first-name-tf")
					label(class="errSup", id="reg-first-name-cf", style="display:none") Enter your first name
				fieldset(class="gridSup")
					label Last Name
					input(class="ipSup", tabindex="2", name="reg-last-name-tf", id="reg-last-name-tf")
					label(class="errSup", id="reg-last-name-cf", style="display:none") Enter your last name
				fieldset(class="gridSup")
					label Username
					input(class="ipSup", tabindex="3", name="reg-user-name-su-tf", id="reg-user-name-su-tf")
					label(class="errSup", id="reg-user-name-su-cf", style="display:none") Enter your username
				fieldset(class="gridSup")
					label Email
					input(class="ipSup", tabindex="4", name="reg-email-tf", id="reg-email-tf")
					label(class="errSup", id="reg-email-cf", style="display:none") Enter your email
				fieldset(class="gridSup")
					label Password
					input(type="password", class="ipSup", tabindex="5", name="reg-pass-tf", id="reg-pass-tf")
					label(class="errSup", id="reg-pass-cf", style="display:none") Enter your password
				fieldset(class="gridSup", style="display:none")
					label Location
					input(class="ipSup", tabindex="6", name="reg-location-tf", id="reg-location-tf")
					.form_register_location_au(style="display:none")
						-	for(var i=0;i<resLocation.length;i++)
								input(type="hidden", class="item_location", value=""+resLocation[i].lo_name+"")
				.ckService
					h5 What are you looking for?
					ul(class="lst_ckSup")
						li(class="ck_sup")
							input(type="radio", id="buySer", name="f_type", value="1")
							label(for="buySer") Buy Services
						li(class="ck_sup")
							input(type="radio", id="selSer", name="f_type", value="2", checked)
							label(for="selSer") Sell Services
						li(class="ck_sup", style="display:none")
							input(type="radio", id="both_ckSup", name="f_type", value="0")
							label(for="both_ckSup") Both
				fieldset
					button(type="submit", class="btnsubmit", id="rp-bt-action-form") Sign up
